    "expo-google-app-auth": "^8.0.0",
    "expo-intent-launcher": "~8.1.0",
    "expo-location": "~8.1.0",
    "expo-module-scripts": "^1.1.1",
    "expo-permissions": "~8.1.0",
    "expo-task-manager": "~8.1.0",
